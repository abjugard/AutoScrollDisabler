{
    "manifest_version": 2,
    "name": "AutoScroll Disabler",
    "description": "Stops the annoying autoscroll from activating on middle mouse click",
    "author": "Adrian Bjug√•rd",
    "homepage_url": "https://github.com/abjugard/AutoScrollDisabler",
    "version": "1.3.3.7",
    "content_scripts":
    [
        {
            "matches":
            [
                "<all_urls>"
            ],
            "js":
            [
                "main.js"
            ]
        }
    ],
    "permissions":
    [
        "<all_urls>"
    ]
}